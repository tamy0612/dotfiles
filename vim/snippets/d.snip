snippet import
  import ${1:header}

snippet unittest
  unittest{
    ${1:code}
  }

snippet version
  version( ${1:version} ){
    ${2:code}
  }

snippet main
  void main( string[] args ){
    ${1:code}
  }

snippet hello
  import std.stdio : writeln;

  void main( string[] args ){
    writeln( "Hello, world!" );
  }
